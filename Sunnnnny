MiscTab:CreateButton({
    Name = "Destroy GUI",
    Callback = function()
        -- Disable all features and destroy the GUI
        autoCast = false
        autoReel = false
        autoShake = false
        ZoneCast = false
        AntiDrown = false
        CollarPlayer = false

        if safePart then
            safePart:Destroy()
            safePart = nil
        end

        Neverlose:Destroy()
    end,
})
